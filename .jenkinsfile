pipeline
{
agent any
stages
{
stage('Build')
{
steps{
sh "mvn clean"
}
}
stage('Deploy')
{
steps{
echo "Deploying"
}
}
stage('Test')
{
steps{
sh "mvn test"
}
}
}
post {
    always {
      publishHTML(output: [
        allowMissing: false,
        alwaysLinkToLastBuild: true,
        keepAll: true,

        reportFiles: 'report.html',
        reportName: 'Extent Reports',
        reportTitles: 'Automation-test-reports'
      ])
    }
  }
}
