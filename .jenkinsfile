pipeline
{
agent any
stages
{
stage('Build')
{
steps{
sh "mvn clean"
}
}
stage('Deploy')
{
steps{
echo "Deploying"
}
}
stage('Test')
{
steps{
sh "mvn test"
}
}
}
post {
    always {
      publishHTML([
        allowMissing: false,
        alwaysLinkToLastBuild: true,
        keepAll: true,
        reportDir: "output/report.html",
        reportFiles: 'overview-features.html',
        reportName: 'Extent Reports',
        reportTitles: 'Automation-test-reports'
      ])
    }
  }
}
